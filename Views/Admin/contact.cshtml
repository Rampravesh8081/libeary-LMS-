
@{
    ViewBag.Title = "contact";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<div class="container">
    <div class="row my-4">
        <h2 class="text-center text-primary">Manage Contact</h2>
        <div class="col-sm-12 ">

            <table class="table table-bordered table-hover" id="myTable">
                <thead class="table-dark">
                    <tr>
                        <th>sr</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Email</th>
                        <th>Number </th>
                        <th>Subject</th>
                        <th>Course</th>
                        <th>Message</th>


                    </tr>
                </thead>
                <tbody>
                    @if(ViewBag.table2.Rows.Count > 0)
                    {
                        foreach (var row in ViewBag.table2.Rows)
                        {


                            <tr>
                                <td>@row[0]</td>
                                <td>@row[1]</td>
                                <td>@row[2]</td>
                                <td>@row[3]</td>
                                <td>@row[4]</td>
                                <td>@row[5]</td>
                                <td>@row[6]</td>
                                <td>@row[7]</td>

                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>

